{
  "name": "todo-manager",
  "version": "1.0.0",
  "scripts": {
    "start": "nodemon -e js,ejs",
    "start:prod": "node index.js",
    "db:create:test": "cross-env NODE_ENV=test sequelize-cli db:create",
    "pretest": "cross-env NODE_ENV=test sequelize-cli db:drop && cross-env NODE_ENV=test sequelize-cli db:create",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles",
    "migrate": "sequelize-cli db:migrate",
    "seed": "sequelize-cli db:seed:all",
    "prepare": "is-ci || husky install"
  },
  "engines": {
    "node": "20.x"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "cookie-parser": "^1.4.7",
    "csurf": "^1.11.0",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "method-override": "^3.0.0",
    "pg": "^8.16.3",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.33.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "is-ci": "^3.0.1",
    "jest": "^30.0.5",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4"
  }
}
